skip_list:
  - experimental
  - 'yaml'
  - unnamed-task
  - ignore-errors
  - no-changed-when
warn_list:
  - command-instead-of-shell
  - risky-file-permissions
  - literal-compare
  - meta-incorrect
  - meta-no-info
  - new-line-at-end-of-file
  - trailing-spaces
  - jinja[spacing]
  - fqcn[action-core]
  - schema[meta]
  - name[template]
  - key-order[task]
  - name[casing]
